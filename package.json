{
  "name": "vscode-spectra-assist",
  "displayName": "vscode-spectra-assist",
  "description": "basic syntax highlighting for spectra input files",
  "version": "0.3.0",
  "repository": "https://github.com/seanfroste/vscode-spectra-assist",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "in",
        "aliases": [
          "spectra-in",
          "in"
        ],
        "extensions": [
          ".in"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "in",
        "scopeName": "source.in",
        "path": "./syntaxes/in.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "in",
        "path": "./snippets/in.code-snippets"
      }
    ]
  }
}